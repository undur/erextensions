<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXLocalizedString" wocomponentcontent="false">

		<binding name="value"/>
		<binding name="object"/>
		<binding name="keyPath"/>
		<binding name="valueWhenEmpty"/>
		<binding name="templateString"/>
		<binding defaults="Boolean" name="escapeHTML"/>

		<validation message="&quot;You must bind at least one of object, value or keyPath&quot;">
			<and>
				<unbound name="value"/>
				<unbound name="object"/>
				<unbound name="keyPath"/>
			</and>
		</validation>
	<binding name="otherObject"/>
        <binding defaults="Boolean" name="omitWhenEmpty"/>
    </wo>
</wodefinitions>
