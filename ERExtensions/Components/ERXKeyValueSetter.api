<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXKeyValueSetter" wocomponentcontent="true">
 
		<binding name="source"/>
		<binding name="destination"/>
		<binding name="sourceKeypath"/>
		<binding name="destinationKeypath"/>

		<validation message="'source' is a required binding">
			<unbound name="source"/>
		</validation>

		<validation message="'destination' is a required binding">
			<unbound name="destination"/>
		</validation>
	</wo>
</wodefinitions>