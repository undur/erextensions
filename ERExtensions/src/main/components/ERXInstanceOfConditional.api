<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERXInstanceOfConditional" wocomponentcontent="true">

		<binding name="object"/>
		<binding name="className"/>
		<binding name="negate" defaults="Boolean"/>

		<validation message="'object' is a required binding">
			<unbound name="object"/>
		</validation>

		<validation message="'className' is a required binding">
			<unbound name="className"/>
		</validation>
	</wo>
</wodefinitions>